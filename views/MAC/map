function(doc) {
    if(doc.networks.length>0&&parseInt(doc.point.acc)<100){
var geometry= {"type": "Point",coordinates:[parseFloat(doc.point.loc.lng),parseFloat(doc.point.loc.lat),parseInt(doc.point.alt)]};
        doc.networks.forEach(function(m){
            var out ={geometry:geometry};
            out.strength=parseInt(m.strength);
            out.SSID=m.SSID;
            out.MAC=m.MAC;
            emit(m.MAC,out);
        })
    }
}
